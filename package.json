{
  "name": "exchangenest-backend",
  "version": "1.0.0",
  "description": "API Rest for the ExchangeNest application",
  "engines": {
    "node": "~18.19.0",
    "npm": "~10.2.5"
  },
  "main": "./build/src/apps/pie/start.js",
  "scripts": {
    "dev:pie": "SET NODE_ENV=development & tsnd --ignore-watch node_modules ./src/apps/pie/start.ts",
    "test": "npm run test:unit && npm run test:features",
    "test:unit": "SET NODE_ENV=test & jest",
    "test:features": "npm run test:pie:features",
    "test:pie:features": "SET NODE_ENV=test & cucumber-js -p pie",
    "lint": "tslint src/**/*.ts{,x}",
    "start:pie": "SET NODE_ENV=production & node ./build/src/apps/pie/start.js",
    "build": "rm -r build; exit 0 && npm run build:tsc && copy 'src/**/*.{json,yaml,html,png}' build/src && npm run build:config",
    "build:w": "npm run build:clean && npm run build:tsc && npm run build:di && npm run build:config",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "build:di": "xcopy .\\src\\apps\\pie\\dependency-injection .\\build\\src\\apps\\pie\\dependency-injection /ise",
    "build:config": "xcopy .\\src\\Contexts\\PIE\\Shared\\infrastructure\\config .\\build\\src\\Contexts\\PIE\\Shared\\infrastructure\\config /ise",
    "build:clean": "rd /s /q build",
    "start": "ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmanuelPatricio/ExchangeNest-Backend.git"
  },
  "keywords": [
    "exchange",
    "program"
  ],
  "author": "Emanuel Patricio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EmanuelPatricio/ExchangeNest-Backend/issues"
  },
  "homepage": "https://github.com/EmanuelPatricio/ExchangeNest-Backend#readme",
  "dependencies": {
    "@prisma/client": "5.7.0",
    "body-parser": "1.20.2",
    "bodybuilder": "2.5.1",
    "bson": "6.2.0",
    "compression": "1.7.4",
    "connect-flash": "0.1.1",
    "convict": "6.2.4",
    "cookie-parser": "1.4.6",
    "cookie-session": "2.0.0",
    "copy": "0.0.1",
    "crypto-js": "4.2.0",
    "dotenv": "16.3.1",
    "errorhandler": "1.5.1",
    "express": "4.18.2",
    "express-promise-router": "4.1.1",
    "express-validator": "7.0.1",
    "glob": "10.3.10",
    "helmet": "7.1.0",
    "http-status": "1.7.3",
    "node-dependency-injection": "2.6.11",
    "nunjucks": "3.2.4",
    "pg": "8.11.3",
    "reflect-metadata": "0.1.13",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "10.0.1",
    "@types/aws-lambda": "8.10.130",
    "@types/bluebird": "3.5.42",
    "@types/compression": "1.7.5",
    "@types/connect-flash": "0.0.40",
    "@types/convict": "6.1.6",
    "@types/cookie-parser": "1.4.6",
    "@types/cookie-session": "2.0.48",
    "@types/crypto-js": "4.2.1",
    "@types/errorhandler": "1.5.3",
    "@types/express": "4.17.21",
    "@types/faker": "5.5.7",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.10",
    "@types/node": "16.11.10",
    "@types/nunjucks": "3.2.6",
    "@types/supertest": "2.0.16",
    "@types/swagger-ui-express": "4.1.6",
    "faker": "5.5.3",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "15.2.0",
    "mongodb": "5.9.2",
    "prettier": "3.1.0",
    "prisma": "5.7.0",
    "supertest": "6.3.3",
    "swagger-autogen": "2.23.7",
    "swagger-ui-express": "5.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.7.0",
    "ts-node-dev": "2.0.0",
    "typescript": "5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
